class ReverseString {
        public static void reverse(char[] s) {
            char[] arr=new char[s.length];
            int j=0;
            for(int i=s.length-1;i>=0;i--){
                arr[i]=s[j];
                j++;
            }
                for(int k=0;k<arr.length;k++){
                    System.out.print(arr[k]);
                }
        }
        public static void main(String args[]){
            char[] s={'a','e','i','o','u'};
            reverse(s);
        }
    }
